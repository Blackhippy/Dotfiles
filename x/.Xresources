URxvt*font: xft:peep:size=12
URxvt*boldfont: xft:peep:size=12
Xft.dpi: 96

!Xft.autohint: 0
!Xft.antialias: 1
!Xft.hinting: true
!Xft.hintstyle: hintslight
!Xft.rgba: rgb
!Xft.lcdfilter: lcddefault
URxvt.scrollBar: false
URxvt.internalBorder: 20
URxvt*dynamicColors: on
URxvt*lineSpace: 2
URxvt.geometry: 72x16

! special
URxvt*.foreground:   #c2cacc
URxvt*.background:   [100]#000000
URxvt*.cursorColor:  #c2cacc
URxvt*borderColor:  [100]#000000

! black
*.color0:       #6da4a4
*.color8:       #6da4a4

! red
*.color1:       #8ed6d6
*.color9:       #8ed6d6

! green
*.color2:       #9a9dab
*.color10:      #9a9dab

! yellow
*.color3:       #c7cadd
*.color11:      #c7cadd

! blue
*.color4:       #ad7b96
*.color12:      #ad7b96

! magenta
*.color5:       #df9ec1
*.color13:      #df9ec1

! cyan
*.color6:       #c39caf
*.color14:      #c39caf

! white
*.color7:       #f5c4db
*.color15:      #f5c4db


! Black color that will not be affected by bold highlighting.
*.color66: #000000
*color66:  #000000

! 2BWM
twobwm.border_width: 	   5
twobwm.outer_border: 2
twobwm.inverted_colors:    false
twobwm.enable_compton: true

twobwm.focus_color: #f5c4db
twobwm.unfocus_color: #f5c4db
twobwm.fixed_color: #f5c4db
twobwm.unkill_color: #f5c4db
twobwm.outer_border_color: #ad7b96
twobwm.fixed_unkill_color: #ab3f47 !color5



