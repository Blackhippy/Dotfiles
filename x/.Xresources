!****
#include ".Xresources.colors"
! foreground
*foreground: wht

! background
*background: bg
! blk
*color0:  blk

! gry
*color8:  gry

! red
*color1:  red
*color5:  red
*color9:  red
*color13: red

! grn
*color2:  grn
*color3:  grn
*color10: grn
*color11: grn

! blu
*color4:  blu
*color6:  blu
*color12: blu
*color14: blu

! wht
*color7:  wht
*color15: wht
