Xft.antialias: 1
Xft.hinting: 1
Xft.hintstyle: hintslight
Xft.rgba: rgb
Xft.lcdfilter: lcddefault

!****
!st settings
!****
st.termname: st-256color
st.shell: /bin/zsh
st.font:PragmataPro:pixelsize=16:antialias=true:autohint=true;
st.bold_font: 0
!st.bold_font: Calling Code:pixelsize=12;
st.borderpx: 100
st.cursorshape: 1;
st.cursorthickness: 2
st.cursorblinkstyle: 1
st.xfps: 120
st.actionfps: 120
st.mouseScrollLines: 1
st.opacity: 255
st.chscale: 1.0
st.cwscale: 1.0
