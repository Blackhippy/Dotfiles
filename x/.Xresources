! Black color that will not be affected by bold highlighting.
*.color66: #080B0D
*color66:  #080B0D


! 2BWM
twobwm.border_width: 	   10
twobwm.outer_border: 0
twobwm.inverted_colors:    false
twobwm.enable_compton: true

! 2BWM COLORS
twobwm.focus_color: #728D9C !color11
twobwm.unfocus_color: #88788B !color10
twobwm.fixed_color: #8CA5B3 !color4
twobwm.unkill_color: #5F7887 !color1
twobwm.outer_border_color: #080B0D !color0
twobwm.fixed_unkill_color: #9FB8C7 !color5


! ST Settings
st.font:  tewii:size8
st.termname:                    st-256color
st.shell:                       /bin/zsh
st.tabspaces:                   16
st.chscale:                     1.0
st.cwscale:                     0.2

st.foreground:   #f19f79
st.background:   #0f0202
st.cursorColor:  #f19f79

