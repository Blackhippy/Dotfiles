!URxvt*font: xft:lucy tewii:size=8,xft:Font Awesome:size=9
!URxvt*font: xft:hermit:size=8, xft:Font 
URxvt*font: xft:ypn envypn:size=8, xft:Font
URxvt*boldfont: xft:ypn envypn:size=8, xft:Font
Awesome:size=9, xft:powerline:size=9
!URxvt*boldFont: xft:lucy tewii:size=8
Xft.dpi: 96
Xft.autohint: 0
Xft.antialias: 1
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.rgba: rgb
Xft.lcdfilter: lcddefault
URxvt.scrollBar: false
URxvt.internalBorder: 20
URxvt*dynamicColors: on
URxvt*lineSpace: 2

! Colors 0-15.
URxvt*foreground:   #dedfe0
URxvt*background:   [100]#1e1718
URxvt*cursorColor:  #dedfe0
URxvt*borderColor:  [100]#1e1718

! Colors 0-15.
*.color0: #1e1718
*color0:  #1e1718
*.color1: #A69299
*color1:  #A69299
*.color2: #B2AEB4
*color2:  #B2AEB4
