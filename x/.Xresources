URxvt*font: xft:PixelCarnageMonoTT:size=10, xft:Font Awesome:size=9
URxvt*boldfont: xft:PixelCarnageMonoTT:size=10,xft:Font Awesome:size=9
!URxvt*font: xft:hermit:size=8, xft:Font
!URxvt*font: xft:ypn envypn:size=8, xft:Font
!URxvt*boldfont: xft:ypn envypn:size=8, xft:FontAwesome:size=9
Xft.dpi: 96
Xft.autohint: 0
Xft.antialias: 1
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.rgba: rgb
Xft.lcdfilter: lcddefault
URxvt.scrollBar: false
URxvt.internalBorder: 20
URxvt*dynamicColors: on
URxvt*lineSpace: 2
URxvt.geometry: 72x16

! Colors 0-15.
URxvt*foreground:   #d0dce2
URxvt*background:   [100]#080B0D
URxvt*cursorColor:  #d0dce2
URxvt*borderColor:  [100]#080B0D

! Colors 0-15.
*.color0: #080B0D
*color0:  #080B0D
*.color1: #5F7887
*color1:  #5F7887
*.color2: #88788B
*color2:  #88788B
*.color3: #728D9C
*color3:  #728D9C
*.color4: #8CA5B3
*color4:  #8CA5B3
*.color5: #9FB8C7
*color5:  #9FB8C7
*.color6: #C6B8CB
*color6:  #C6B8CB
*.color7: #d0dce2
*color7:  #d0dce2
*.color8: #919a9e
*color8:  #919a9e
*.color9: #5F7887
*color9:  #5F7887
*.color10: #88788B
*color10:  #88788B
*.color11: #728D9C
*color11:  #728D9C
*.color12: #8CA5B3
*color12:  #8CA5B3
*.color13: #9FB8C7
*color13:  #9FB8C7
