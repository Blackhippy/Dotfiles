URxvt*font: xft:PixelCarnageMonoTT:size=10, xft:Font Awesome:size=9
URxvt*boldfont: xft:PixelCarnageMonoTT:size=10,xft:Font Awesome:size=9
!URxvt*font: xft:hermit:size=8, xft:Font
!URxvt*font: xft:ypn envypn:size=8, xft:Font
!URxvt*boldfont: xft:ypn envypn:size=8, xft:FontAwesome:size=9
Xft.dpi: 96
!Xft.autohint: 0
!Xft.antialias: 1
!Xft.hinting: true
!Xft.hintstyle: hintslight
!Xft.rgba: rgb
!Xft.lcdfilter: lcddefault
URxvt.scrollBar: false
URxvt.internalBorder: 20
URxvt*dynamicColors: on
URxvt*lineSpace: 2
URxvt.geometry: 72x16

! COLORS
! special
*.foreground:   #ffce8f
*.background:   #16171a
*.cursorColor:  #ffce8f

! black
*.color0:       #2d2f36
*.color8:       #444752

! red
*.color1:       #9f6436
*.color9:       #c4824f

! green
*.color2:       #da9832
*.color10:      #e6b363

! yellow
*.color3:       #a97c32
*.color11:      #cb9c4d

! blue
*.color4:       #b3792e
*.color12:      #d3974b

! magenta
*.color5:       #b38949
*.color13:      #c8a671

! cyan
*.color6:       #bf8946
*.color14:      #d1a671

! white
*.color7:       #eddac4
*.color15:      #fbf8f3

! 2BWM
twobwm.border_width: 	   10
twobwm.outer_border: 0
twobwm.inverted_colors:    false
twobwm.enable_compton: true

! 2BWM COLORS
twobwm.focus_color: #cb9c4d !color11
twobwm.unfocus_color: #e6b363 !color10
twobwm.fixed_color: #b3792e !color4
twobwm.unkill_color: #9f6436 !color1
twobwm.outer_border_color: #2d2f36 !color0
twobwm.fixed_unkill_color: #b38949 !color5


! ST Settings
st.font:  tewii:size8
st.termname:                    st-256color
st.shell:                       /bin/zsh
st.tabspaces:                   16
st.chscale:                     1.0
st.cwscale:                     0.2
st.foreground:   #f19f79
st.background:   #0f0202
st.cursorColor:  #f19f79

