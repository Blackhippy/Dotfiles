! Black color that will not be affected by bold highlighting.
*.color66: #1e1718
*color66:  #1e1718


! 2BWM 
twobwm.border_width: 	   10
twobwm.outer_border: 0
twobwm.inverted_colors:    false
twobwm.enable_compton: true

! 2BWM COLORS
twobwm.focus_color: #CFAAA6
twobwm.unfocus_color: #B2AEB4
twobwm.fixed_color: #DCD1AE
twobwm.unkill_color: #A69299
twobwm.outer_border_color: #CDBCC4
twobwm.fixed_unkill_color: #1E1718


! ST Settings
st.font:  tewii:size8
st.termname:                    st-256color
st.shell:                       /bin/zsh
st.tabspaces:                   16
st.chscale:                     1.0
st.cwscale:                     0.2

st.foreground:   #f19f79
st.background:   #0f0202
st.cursorColor:  #f19f79

