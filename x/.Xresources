Xft.autohint: 0
Xft.antialias: 1
Xft.hinting: 1
Xft.hintstyle: hintslight
Xft.rgba: rgb
Xft.lcdfilter: lcddefault

!****
!st settings
!****
st.termname: st-256color
st.shell: /bin/zsh
st.font:Sometype Mono:pixelsize=15:antialias=true:autohint=true;
st.bold_font: Victor Mono:Bold Italic:pixelsize=15:antialias=true:autohint=true;
!st.bold_font: Monoid Tight:pixelsize=12:Bold:antialias=true:autohint=true;
!st.bold_font: Calling Code:pixelsize=12;
st.borderpx: 100
st.cursorshape: 1;
st.cursorthickness: 2
st.cursorblinkstyle: 1
st.xfps: 120
st.actionfps: 120
st.mouseScrollLines: 1
st.opacity: 255
st.chscale: 1.0
st.cwscale: 1.0

