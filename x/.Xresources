URxvt*font: xft:leggie:size=10
!URxvt*font: xft:cherry:size=10
URxvt*boldfont: xft:leggiey:size=10
!URxvt*font: xft:PixelCarnageMonoTT:size=10, xft:Font Awesome:size=9
!URxvt*boldfont: xft:PixelCarnageMonoTT:size=10,xft:Font Awesome:size=9
!URxvt*font: xft:hermit:size=8, xft:Font
!URxvt*font: xft:ypn envypn:size=8, xft:Font
!URxvt*boldfont: xft:ypn envypn:size=8, xft:FontAwesome:size=9
Xft.dpi: 96

!Xft.autohint: 0
!Xft.antialias: 1
!Xft.hinting: true
!Xft.hintstyle: hintslight
!Xft.rgba: rgb
!Xft.lcdfilter: lcddefault
URxvt.scrollBar: false
URxvt.internalBorder: 20
URxvt*dynamicColors: on
URxvt*lineSpace: 2
URxvt.geometry: 72x16

URxvt*foreground:   #e9c1c5
URxvt*background:   [100]#260000
URxvt*cursorColor:  #e9c1c5
URxvt*borderColor:  [100]#260000

