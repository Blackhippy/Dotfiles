! 2BWM
twobwm.border_width: 	   10
twobwm.outer_border: 0
twobwm.inverted_colors:    false
twobwm.enable_compton: true

! 2BWM COLORS
twobwm.focus_color: #e9c1c5 !color11
twobwm.unfocus_color: #e9c1c5 !color10
twobwm.fixed_color: #e9c1c5 !olor4
twobwm.unkill_color: #e9c1c5 !color1
twobwm.outer_border_color: #e9c1c5 !color0
twobwm.fixed_unkill_color: #e9c1c5 !color5

!twobwm.focus_color: #a9ae8b !color11
!twobwm.unfocus_color: #91a991 !color10
!twobwm.fixed_color: #66715e !color4
!twobwm.unkill_color: #98905c !color1
!twobwm.outer_border_color: #3a3219 !color0
!twobwm.fixed_unkill_color: #6b6f54 !color5


