zURxvt*font: xft:leggie:size=10
!URxvt*font: xft:cherry:size=10
URxvt*boldfont: xft:cherry:size=10
!URxvt*font: xft:PixelCarnageMonoTT:size=10, xft:Font Awesome:size=9
!URxvt*boldfont: xft:PixelCarnageMonoTT:size=10,xft:Font Awesome:size=9
!URxvt*font: xft:hermit:size=8, xft:Font
!URxvt*font: xft:ypn envypn:size=8, xft:Font
!URxvt*boldfont: xft:ypn envypn:size=8, xft:FontAwesome:size=9
Xft.dpi: 96

!Xft.autohint: 0
!Xft.antialias: 1
!Xft.hinting: true
!Xft.hintstyle: hintslight
!Xft.rgba: rgb
!Xft.lcdfilter: lcddefault
URxvt.scrollBar: false
URxvt.internalBorder: 20
URxvt*dynamicColors: on
URxvt*lineSpace: 2
URxvt.geometry: 72x16


! COLORS
! special
*.foreground:   #86c4e9
*.background:   #000000
*.cursorColor:  #86c4e9

! black
*.color0:       #1a1a1a
*.color8:       #333333

! red
*.color1:       #1c73b0
*.color9:       #2c94de

! green
*.color2:       #1e72a8
*.color10:      #2a95d9

! yellow
*.color3:       #1f71a3
*.color11:      #2895d7

! blue
*.color4:       #2f75a8
*.color12:      #4895cd

! magenta
*.color5:       #23729f
*.color13:      #2e96d2

! cyan
*.color6:       #0587b8
*.color14:      #06b3f4

! white
*.color7:       #c2ddef
*.color15:      #f3f8fc




! 2BWM
twobwm.border_width: 	   10
twobwm.outer_border: 0
twobwm.inverted_colors:    false
twobwm.enable_compton: true

! 2BWM COLORS
twobwm.focus_color: #ffffff !color11
twobwm.unfocus_color: #ffffff !color10
twobwm.fixed_color: #ffffff !color4
twobwm.unkill_color: #ffffff !color1
twobwm.outer_border_color: #ffffff !color0
twobwm.fixed_unkill_color: #ffffff !color5

!twobwm.focus_color: #a9ae8b !color11
!twobwm.unfocus_color: #91a991 !color10
!twobwm.fixed_color: #66715e !color4
!twobwm.unkill_color: #98905c !color1
!twobwm.outer_border_color: #3a3219 !color0
!twobwm.fixed_unkill_color: #6b6f54 !color5


! ST Settings
st.font:  tewii:size8
st.termname:                    st-256color
st.shell:                       /bin/zsh
st.tabspaces:                   16
st.chscale:                     1.0
st.cwscale:                     0.2
st.foreground:   #aab6c5
st.background:   #13191c
st.cursorColor:  #aab6c5

