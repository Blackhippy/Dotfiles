URxvt*font: xft:ProFont:size=10
URxvt*boldfont: xft:gelly:size=10 
Xft.dpi: 96

!Xft.autohint: 0
!Xft.antialias: 1
!Xft.hinting: true
!Xft.hintstyle: hintslight
!Xft.rgba: rgb
!Xft.lcdfilter: lcddefault
URxvt.scrollBar: false
!URxvt.internalBorder: 20
URxvt*dynamicColors: on
URxvt*lineSpace: 2
URxvt.geometry: 72x16

! special
URxvt*.foreground:   #c2cacc
URxvt*.background:   [100]#000000
URxvt*.cursorColor:  #c2cacc
URxvt*borderColor:  [100]#000000

! black
*.color0:       #a7a5a9
*.color8:       #d8d5db

! red
*.color1:       #c3c2c3
*.color9:       #f5f3f5

! green
*.color2:       #ad989d
*.color10:      #dfc3ca

! yellow
*.color3:       #ad7e84
*.color11:      #dfa2aa

! blue
*.color4:       #ac5d65
*.color12:      #de7882

! magenta
*.color5:       #ab2834
*.color13:      #dd3343

! cyan
*.color6:       #ab3f47
*.color14:      #dd515b

! white
*.color7:       #c4232e
*.color15:      #f62b39

! Black color that will not be affected by bold highlighting.
*.color66: #000000
*color66:  #000000

! 2BWM
twobwm.border_width: 	   2
twobwm.outer_border: 5
twobwm.inverted_colors:    false
twobwm.enable_compton: true

twobwm.focus_color: #ab3f47 !color11
twobwm.unfocus_color: #ab3f47  !color10
twobwm.fixed_color: #ab3f47  !color4
twobwm.unkill_color: #ab3f47  !color1
twobwm.outer_border_color: #ac5d65 !color0
twobwm.fixed_unkill_color: #ab3f47 !color5



